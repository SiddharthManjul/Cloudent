{
  "name": "cloudent-reputation-circuit",
  "version": "1.0.0",
  "description": "AI Agent Reputation Zero-Knowledge Proof Circuit",
  "main": "dist/prover.js",
  "scripts": {
    "build": "tsc",
    "compile": "circom circuits/cloudent.circom --r1cs --wasm --sym -o build -l node_modules/circomlib/circuits",
    "setup": "./scripts/setup.sh",
    "prove": "npm run build && node dist/prover.js",
    "prove:input": "npm run build && node dist/prover.js input.json",
    "verify": "npm run build && node dist/verifier.js",
    "witness": "node build/cloudent_js/generate_witness.js build/cloudent_js/cloudent.wasm input.json witness.wtns",
    "clean": "rm -rf build dist ptau *.wtns *.json proof.json public.json calldata.txt",
    "test": "npm run compile && npm run setup && npm run prove",
    "dev": "ts-node src/prover.ts"
  },
  "keywords": [
    "zero-knowledge",
    "circom",
    "snarkjs",
    "reputation",
    "ai-agent",
    "blockchain"
  ],
  "dependencies": {
    "@types/snarkjs": "^0.7.9",
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Cloudent Team",
  "license": "MIT"
}